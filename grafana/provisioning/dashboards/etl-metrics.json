{
  "id": null,
  "title": "ETL Metrics Dashboard",
  "tags": ["etl", "metrics"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "ETL Jobs Status",
      "type": "stat",
      "targets": [
        {
          "expr": "etl_jobs_in_progress",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "ETL Job Duration",
      "type": "graph",
      "targets": [
        {
          "expr": "etl_job_duration_seconds",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "HTTP Requests",
      "type": "graph",
      "targets": [
        {
          "expr": "http_requests_total",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s"
}
